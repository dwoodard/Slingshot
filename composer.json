{
    "name": "dwoodard/slingshot",
    "description": "Slingshot is a library that will set up a new Laravel with In a very opinionated setup of Laradock, Vue, Inertia, Vuetify, with common composer packages",
    "license": "proprietary",
    "authors": [
        {
            "name": "Dustin Woodard",
            "email": "dustin.woodard@gmail.com",
            "homepage": "https://github.com/dwoodard/slingshot-dev"
        }
    ],
    "homepage": "https://github.com/dwoodard/slingshot",
    "keywords": [
        "Laravel",
        "Slingshot"
    ],
    "require": {
        "inertiajs/inertia-laravel": "dev-master",
        "wulfheart/pretty_routes": "dev-master",
        "silviolleite/laravelpwa": "~2.0.3",
        "spatie/laravel-schemaless-attributes": "^1.8",
        "spatie/laravel-permission": "^4.2",
        "tightenco/ziggy": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Dwoodard\\Slingshot\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dwoodard\\Slingshot\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dwoodard\\Slingshot\\SlingshotServiceProvider"
            ],
            "aliases": {
                "Slingshot": "Dwoodard\\Slingshot\\Facades\\Slingshot"
            }
        }
    }
}
